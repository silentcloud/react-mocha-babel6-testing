{
  "name": "react-mocha-babel6-testing",
  "version": "0.0.2",
  "description": "Unit testing React with Mocha & jsdom & babel6",
  "author": "rjmuqiang@gmail.com",
  "keywords": [
    "react",
    "babel6",
    "mocha",
    "testing"
  ],
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-istanbul": "^0.5.9",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "jsdom": "^7.2.1",
    "mocha": "~2.3.4",
    "react-addons-test-utils": "~0.14.0",
    "react-dom": "^0.14.3"
  },
  "dependencies": {
    "react": "~0.14.3"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js --recursive",
    "test:watch": "mocha --compilers js:babel-core/register --require ./test/setup.js --recursive --watch",
    "cover": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- --recursive"
  },
  "license": "ISC"
}
